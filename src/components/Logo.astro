<style>
  .logo-svg {
    height: 100%;
    width: auto;
  }

  /* Primary color (verde) para icono, O, y "mega" */
  .logo-icon,
  .logo-omega-o,
  .logo-mega {
    color: var(--color-primary, #65a344);
  }

  /* Secondary color (naranja/terracota) para "salud" */
  .logo-salud {
    color: var(--color-secondary, #c47f3a);
  }

  /* Inner parts - color de fondo para contraste */
  .logo-inner {
    color: var(--color-base-100, #fafafa);
  }

  /* Cuando est√° en navbar transparente (fondo oscuro), todo blanco */
  .logo-svg[data-invert="true"] .logo-icon,
  .logo-svg[data-invert="true"] .logo-omega-o,
  .logo-svg[data-invert="true"] .logo-mega,
  .logo-svg[data-invert="true"] .logo-salud {
    color: white;
  }

  .logo-svg[data-invert="true"] .logo-inner {
    color: rgba(255, 255, 255, 0.15);
  }
</style>
